<Window x:Class="WpfFrmRound.MainWindow"
xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
xmlns:local="clr-namespace:WpfFrmRound"
mc:Ignorable="d"
Title="MainWindow" Height="450" Width="450">
    <Grid>

        <Ellipse Margin="30" Fill="White" Stroke="Black" StrokeThickness="1" />
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Height="181">
            <DatePicker x:Name="dtpStart" Margin="8"/>
            <DatePicker x:Name="dtpFinish" Margin="8"/>
            <TextBox x:Name="tbResult" Margin="8"/>
            <Button x:Name="btnCalc" Content="Вычислить" Width="98" Click="btnCalc_Click" Margin="8"/>
        </StackPanel>
    </Grid>
</Window>